{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install && npx prisma generate",
  "outputDirectory": "build/client",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/build/server/index.js" }
  ],
  "functions": {
    "build/server/index.js": {
      "includeFiles": "node_modules/**"
    }
  }
}